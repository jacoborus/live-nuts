{
  "name": "nuts",
  "version": "0.0.1",
  "description": "Live HTML template engine using NUTS syntax",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint src test",
    "test": "npm run build && node_modules/run-headless-chromium/run-headless-chromium.js test/test.html | faucet",
    "build": "browserify src/live-nuts.js --standalone Nuts -o dist/live-nuts.js && browserify test/index.js -o dist/tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jacoborus/live-nuts.git"
  },
  "keywords": [
    "NUTS",
    "html",
    "live-templates",
    "templates",
    "engine"
  ],
  "author": "Jacobo Tabernero - jacobo@xarope.eu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacoborus/live-nuts/issues"
  },
  "homepage": "https://github.com/jacoborus/live-nuts",
  "devDependencies": {
    "browserify": "^11.1.0",
    "eslint": "^1.4.1",
    "faucet": "0.0.1",
    "run-headless-chromium": "0.0.12",
    "tape": "^4.2.0"
  }
}
